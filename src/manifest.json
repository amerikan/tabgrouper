{
    "manifest_version": 2,
    "name": "Tabtastic",
    "version": "1.0.6",
    "description": "Adds a browser button to order tabs by domain and URL, and closing any duplicate tabs.",
    "homepage_url": "https://github.com/amerikan/tabtastic",
    "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "96": "icons/icon96.png",
      "128": "icons/icon128.png"
    },
    "permissions": [
        "tabs"
    ],
    "browser_action": {
      "default_icon": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "96": "icons/icon96.png",
        "128": "icons/icon128.png"
      }
    },
    "background": {
        "scripts": ["background.js"]
    }
  }
